                    <tr>
                        <td colspan="5" style="border: 1px solid black;padding: 5px 6px 5px 0px; text-align: right;font-size:13px">IGST@18%</td>
                        <apex:outputText rendered="{!OR(opp.CurrencyIsoCode == 'USD', opp.CurrencyIsoCode == 'AUD')}">
                            <td style="border: 1px solid black; padding: 4px 0px 4px 20px; text-align: left;font-size:13px">$ {!formatigst}</td>
                        </apex:outputText>
                        <apex:outputText rendered="{!opp.CurrencyIsoCode == 'INR'}">
                            <td style="border: 1px solid black; padding: 4px 0px 4px 20px; text-align: left;font-size:13px"><apex:image url="{!$Resource.Rupee_Symbol}" width="10" height="10"/> {!formatigst}</td>
                        </apex:outputText>
                        <apex:outputText rendered="{!opp.CurrencyIsoCode == 'GBP'}">
                            <td style="border: 1px solid black; padding: 4px 0px 4px 20px; text-align: left;font-size:13px">£ {!formatigst}</td>
                        </apex:outputText>
                        <apex:outputText rendered="{!AND(opp.CurrencyIsoCode != 'USD', opp.CurrencyIsoCode != 'INR', opp.CurrencyIsoCode != 'GBP',opp.CurrencyIsoCode != 'AUD')}">
                            <td style="border: 1px solid black; padding: 4px 0px 4px 20px; text-align: left;font-size:13px">{!formatigst}</td>
                        </apex:outputText>
                    </tr>
                </apex:outputText>
                
                <apex:outputText rendered="{!showCGST_SGST}">
                    <tr>
                        <td colspan="5" style="border: 1px solid black; padding:5px 6px 5px 0px; text-align:right;font-size:13px">CGST@9%</td>
                        <apex:outputText rendered="{!OR(opp.CurrencyIsoCode == 'USD', opp.CurrencyIsoCode == 'AUD')}">
                            <td style="border: 1px solid black; padding: 4px 0px 4px 20px; text-align: left;font-size:13px">$ {!formatcgst}</td>
                        </apex:outputText>
                        <apex:outputText rendered="{!opp.CurrencyIsoCode == 'INR'}">
                            <td style="border: 1px solid black; padding: 4px 0px 4px 20px; text-align: left;font-size:13px"><apex:image url="{!$Resource.Rupee_Symbol}" width="10" height="10"/> {!cgst}</td>
                        </apex:outputText>
                        <apex:outputText rendered="{!opp.CurrencyIsoCode == 'GBP'}">
                            <td style="border: 1px solid black; padding: 4px 0px 4px 20px; text-align: left;font-size:13px">£ {!formatcgst}</td>
                        </apex:outputText>
                        <apex:outputText rendered="{!AND(opp.CurrencyIsoCode != 'USD', opp.CurrencyIsoCode != 'INR', opp.CurrencyIsoCode != 'GBP',opp.CurrencyIsoCode != 'AUD')}">
                            <td style="border: 1px solid black; padding: 4px 0px 4px 20px; text-align: left;font-size:13px">{!formatcgst}</td>
                        </apex:outputText>
                    </tr>
                    
                    <tr>
                        <td colspan="5" style="border: 1px solid black;padding: 5px 6px 5px 0px; text-align: right;font-size:13px">SGST@9%</td>
                        <apex:outputText rendered="{!OR(opp.CurrencyIsoCode == 'USD', opp.CurrencyIsoCode == 'AUD')}">
                            <td style="border: 1px solid black; padding: 4px 0px 4px 20px; text-align: left;font-size:13px">$ {!formatsgst}</td>
                        </apex:outputText>
                        <apex:outputText rendered="{!opp.CurrencyIsoCode == 'INR'}">
                            <td style="border: 1px solid black; padding: 4px 0px 4px 20px; text-align: left;font-size:13px"><apex:image url="{!$Resource.Rupee_Symbol}" width="10" height="10"/> {!formatsgst}</td>
                        </apex:outputText>
                        <apex:outputText rendered="{!opp.CurrencyIsoCode == 'GBP'}">
                            <td style="border: 1px solid black; padding: 4px 0px 4px 20px; text-align: left;font-size:13px">£ {!formatsgst}</td>
                        </apex:outputText>
                        <apex:outputText rendered="{!AND(opp.CurrencyIsoCode != 'USD', opp.CurrencyIsoCode != 'INR', opp.CurrencyIsoCode != 'GBP',opp.CurrencyIsoCode != 'AUD')}">
                            <td style="border: 1px solid black; padding: 4px 0px 4px 20px; text-align: left;font-size:13px">{!formatsgst}</td>
                        </apex:outputText>
                    </tr>
                </apex:outputText>
                
            </apex:outputText>
            
            <tr>
                <apex:outputText rendered="{!opp.CurrencyIsoCode == 'INR'}">
                    <td colspan="2" style="border: 1px solid black;padding: 6px; text-align:left;font-size:13px"><span style="font-weight:bold">Currency:</span> INR(Indian Rupees)</td>
                </apex:outputText>
                <apex:outputText rendered="{!opp.CurrencyIsoCode == 'USD'}">
                    <td colspan="2" style="border: 1px solid black;padding: 6px; text-align:left;font-size:13px"><span style="font-weight:bold">Currency:</span> USD(United States dollar)</td>
                </apex:outputText>
                <apex:outputText rendered="{!opp.CurrencyIsoCode == 'GBP'}">
                    <td colspan="2" style="border: 1px solid black;padding: 6px; text-align:left;font-size:13px"><span style="font-weight:bold">Currency:</span> GBP(Great British Pound)</td>
                </apex:outputText>
                <apex:outputText rendered="{!opp.CurrencyIsoCode == 'AUD'}">
                    <td colspan="2" style="border: 1px solid black;padding: 6px; text-align:left;font-size:13px"><span style="font-weight:bold">Currency:</span> AUD(Australian Dollar)</td>
                </apex:outputText>
                
                <td colspan="3" style="border: 1px solid black;padding: 5px 6px 5px 0px; text-align:right;font-weight:bold;font-size:15px">Grand Total</td>
                <apex:outputText rendered="{!OR(opp.CurrencyIsoCode == 'USD', opp.CurrencyIsoCode == 'AUD')}">
                    <td style="border: 1px solid black; padding: 4px 0px 4px 20px; text-align: left;font-weight:bold;font-size:15px">$ {!formattedAmount}</td>
                </apex:outputText>
                <apex:outputText rendered="{!opp.CurrencyIsoCode == 'INR'}">
                    <td style="border: 1px solid black; padding: 4px 0px 4px 20px; text-align: left;font-weight:bold;font-size:15px"><apex:image url="{!$Resource.Rupee_Symbol}" width="10" height="10"/> {!formattedAmount}</td>
                </apex:outputText>
                <apex:outputText rendered="{!opp.CurrencyIsoCode == 'GBP'}">
                    <td style="border: 1px solid black; padding: 4px 0px 4px 20px; text-align: left;font-weight:bold;font-size:15px">£ {!formattedAmount}</td>
                </apex:outputText>
                <apex:outputText rendered="{!AND(opp.CurrencyIsoCode != 'USD', opp.CurrencyIsoCode != 'INR', opp.CurrencyIsoCode != 'GBP',opp.CurrencyIsoCode != 'AUD')}">
                    <td style="border: 1px solid black; padding: 4px 0px 4px 20px; text-align: left;font-weight:bold;font-size:15px">{!formattedAmount}</td>
                </apex:outputText>
            </tr>
            
           
                
        
            
            <tr>
                <td colspan="6" style="border: 1px solid black;padding: 6px; text-align:left;font-size:13px"><span style="font-weight:bold">Amount in Words:</span> {!result}</td>
            </tr>
        </table>
    </div>
    
    
     <apex:outputText rendered="{!NOT(ISBLANK(TRIM(description)))}">
    <p>
        <span style="font-weight:bold;font-size:15px; margin-top:0px">Note:</span>
        <span> {!description}</span>
    </p>
     </apex:outputText>
    
    <div style="width:100%; margin-top:0px;display: inline-block">
        <div style="width:40%; float:left; line-height:0px; margin-top:0px;">
            <apex:outputText rendered="{!NOT(ISBLANK(selectedAccountAddress.Account_No__c))}">
